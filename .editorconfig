# EditorConfig https://editorconfig.org
# Uses editorconfig to maintain consistent coding styles

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[{*.js,*.html}]
indent_size = 2

[*.{tf,tfvars}]
indent_size = 2
indent_style = space

[*.md]
trim_trailing_whitespace = false
